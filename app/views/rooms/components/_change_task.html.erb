<div class="panel panel-default">
  <div class="panel-heading">Change current task from room</div>
    <div class="panel-body">
      <div class='list-group change-task'>
        <% @session_vote.tasks.each do |task|  %>
          <%= link_to task.title, 'javascript:void(0)', class: 'list-group-item item', data: { id: task.id } %>
        <% end %>
      </div>
    </div>
  </div>
</div>
